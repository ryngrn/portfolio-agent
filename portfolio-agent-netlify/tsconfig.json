{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "preserve",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ]
  },
  "include": [
    "app",
    "components",
    "scripts",
    "next-env.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}